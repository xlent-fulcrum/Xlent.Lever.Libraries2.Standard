<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <AssemblyName>Xlent.Lever.Libraries2.Standard</AssemblyName>
    <RootNamespace>Xlent.Lever.Libraries2.Standard</RootNamespace>
    <PostBuildEvent></PostBuildEvent>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard1.4\Xlent.Lever.Libraries2.Standard.xml</DocumentationFile>
  </PropertyGroup>
  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="rd /s /q $(ProjectDir)lib\$(ProjectName)" />
    <Exec Command="mkdir $(ProjectDir)lib\$(ProjectName)\" />
    <Exec Command="copy $(OutDir)Xlent.Lever.Libraries2.Standard.* $(ProjectDir)\lib\$(ProjectName)\" />
  </Target>
</Project>